@{
    ViewBag.Title = "Index";
}
<div id="category-list">
    <h2>
        Category JQGrid Sample</h2>
    <p>
        @Html.ActionLink("Create Category", "Create", "Category", new { @class = "lnkAddCategory button" })
    </p>
    <div id="categorys-jq-grid">
        <table id="categoryJQGrid" class="scroll" cellpadding="0" cellspacing="0">
        </table>
    </div>
    <div id="addCategoryDialog" title="Add Category">
    </div>
    <div id="editCategoryDialog" title="Edit Category">
    </div>
    <div id="deleteCategoryDailog" title="Delete Category">
    </div>
    <div id="detailsCategoryDialog" title="Details Category">
    </div>
</div>
<script src="@Url.Content("~/Scripts/App/category-jq.js")" type="text/javascript"></script>
